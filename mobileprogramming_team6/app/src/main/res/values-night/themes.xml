<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Mobileprogramming_team6" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#ABD5AA</item>
        <item name="colorPrimaryVariant">#E5ECE5</item>
        <item name="colorOnPrimary">#E5ECE5</item>
        <item name="colorControlHighlight">#000000</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <item name="windowNoTitle">true</item>
        <item name="fontFamily">@font/semiboldfont</item>
        <!-- Customize your theme here. -->
    </style>
</resources>